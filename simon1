:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x56282eddaee0 .scope module, "simon1" "simon1" 2 1;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clock"
    .port_info 1 /INPUT 1 "reset"
    .port_info 2 /INPUT 2 "in"
    .port_info 3 /OUTPUT 2 "out"
P_0x56282eddb060 .param/l "B" 1 2 19, C4<010>;
