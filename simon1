v0x56282edfba20_0 .var "state", 2 0;
E_0x56282edcfe20 .event edge, v0x56282edfba20_0, v0x56282edfb660_0;
E_0x56282edcfa40 .event posedge, v0x56282edfb910_0, v0x56282eddb580_0;
    .scope S_0x56282eddaee0;
T_0 ;
    %wait E_0x56282edcfa40;
    %load/vec4 v0x56282edfb910_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 4, 0, 3;
    %assign/vec4 v0x56282edfba20_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x56282edfb740_0;
    %assign/vec4 v0x56282edfba20_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x56282eddaee0;
T_1 ;
    %wait E_0x56282edcfe20;
    %load/vec4 v0x56282edfba20_0;
