#! /usr/bin/vvp
:ivl_version "10.1 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x5645316b9900 .scope module, "simon1" "simon1" 2 1;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clock"
    .port_info 1 /INPUT 1 "reset"
    .port_info 2 /INPUT 2 "in"
    .port_info 3 /OUTPUT 2 "out"
P_0x5645316b9a80 .param/l "B" 1 2 19, C4<010>;
P_0x5645316b9ac0 .param/l "G" 1 2 18, C4<001>;
P_0x5645316b9b00 .param/l "R" 1 2 17, C4<000>;
P_0x5645316b9b40 .param/l "Y" 1 2 20, C4<011>;
P_0x5645316b9b80 .param/l "init" 1 2 21, C4<100>;
P_0x5645316b9bc0 .param/l "lose" 1 2 22, C4<101>;
P_0x5645316b9c00 .param/l "win" 1 2 23, C4<110>;
o0x7f8ff48fc018 .functor BUFZ 1, C4<z>; HiZ drive
v0x5645316b9f20_0 .net "clock", 0 0, o0x7f8ff48fc018;  0 drivers
o0x7f8ff48fc048 .functor BUFZ 2, C4<zz>; HiZ drive
v0x5645316da000_0 .net "in", 1 0, o0x7f8ff48fc048;  0 drivers
v0x5645316da0e0_0 .var "out", 1 0;
o0x7f8ff48fc0a8 .functor BUFZ 1, C4<z>; HiZ drive
v0x5645316da1d0_0 .net "reset", 0 0, o0x7f8ff48fc0a8;  0 drivers
v0x5645316da290_0 .var "state", 2 0;
E_0x5645316b1e20 .event edge, v0x5645316da290_0, v0x5645316da000_0;
E_0x5645316b1a40 .event posedge, v0x5645316da1d0_0, v0x5645316b9f20_0;
    .scope S_0x5645316b9900;
T_0 ;
    %wait E_0x5645316b1a40;
    %load/vec4 v0x5645316da1d0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 4, 0, 3;
    %assign/vec4 v0x5645316da290_0, 0;
    %pushi/vec4 3, 3, 2;
    %cassign/vec4 v0x5645316da0e0_0;
T_0.0 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5645316b9900;
T_1 ;
    %wait E_0x5645316b1e20;
    %load/vec4 v0x5645316da000_0;
    %cmpi/e 3, 3, 2;
    %flag_mov 8, 4;
    %load/vec4 v0x5645316da000_0;
    %cmpi/e 0, 3, 2;
    %flag_or 4, 8;
    %jmp/0xz  T_1.0, 4;
    %pushi/vec4 3, 3, 2;
    %cassign/vec4 v0x5645316da0e0_0;
T_1.0 ;
    %load/vec4 v0x5645316da290_0;
    %cmpi/e 4, 0, 3;
    %jmp/0xz  T_1.2, 4;
    %load/vec4 v0x5645316da000_0;
    %pad/u 3;
    %assign/vec4 v0x5645316da290_0, 0;
    %jmp T_1.3;
T_1.2 ;
    %load/vec4 v0x5645316da290_0;
    %pushi/vec4 0, 0, 3;
    %cmp/e;
    %flag_get/vec4 4;
    %load/vec4 v0x5645316da000_0;
    %pad/u 3;
    %pushi/vec4 0, 0, 3;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
    %flag_set/vec4 8;
    %load/vec4 v0x5645316da290_0;
    %pushi/vec4 2, 0, 3;
    %cmp/e;
    %flag_get/vec4 4;
    %load/vec4 v0x5645316da000_0;
    %pad/u 3;
    %pushi/vec4 2, 0, 3;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
    %flag_set/vec4 9;
    %flag_or 9, 8;
    %load/vec4 v0x5645316da290_0;
    %pushi/vec4 1, 0, 3;
    %cmp/e;
    %flag_get/vec4 4;
    %load/vec4 v0x5645316da000_0;
    %pad/u 3;
    %pushi/vec4 1, 0, 3;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
    %flag_set/vec4 8;
    %flag_or 8, 9;
    %load/vec4 v0x5645316da290_0;
    %pushi/vec4 3, 0, 3;
    %cmp/e;
    %flag_get/vec4 4;
    %load/vec4 v0x5645316da000_0;
    %pad/u 3;
    %pushi/vec4 3, 0, 3;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
    %flag_set/vec4 9;
    %flag_or 9, 8;
    %jmp/0xz  T_1.4, 9;
    %pushi/vec4 2, 0, 2;
    %cassign/vec4 v0x5645316da0e0_0;
    %pushi/vec4 6, 0, 3;
    %assign/vec4 v0x5645316da290_0, 0;
    %jmp T_1.5;
T_1.4 ;
    %pushi/vec4 1, 0, 2;
    %cassign/vec4 v0x5645316da0e0_0;
    %pushi/vec4 5, 0, 3;
    %assign/vec4 v0x5645316da290_0, 0;
T_1.5 ;
T_1.3 ;
    %jmp T_1;
    .thread T_1, $push;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
