#! /usr/bin/vvp
:ivl_version "10.1 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x55acd2ae1f30 .scope module, "simon1" "simon1" 2 1;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clock"
    .port_info 1 /INPUT 1 "reset"
    .port_info 2 /INPUT 2 "in"
    .port_info 3 /OUTPUT 2 "out"
o0x7f0ca6032018 .functor BUFZ 1, C4<z>; HiZ drive
v0x55acd2ae2160_0 .net "clock", 0 0, o0x7f0ca6032018;  0 drivers
o0x7f0ca6032048 .functor BUFZ 2, C4<zz>; HiZ drive
v0x55acd2b02240_0 .net "in", 1 0, o0x7f0ca6032048;  0 drivers
v0x55acd2b02320_0 .var "next_state", 1 0;
v0x55acd2b02410_0 .var "out", 1 0;
o0x7f0ca60320d8 .functor BUFZ 1, C4<z>; HiZ drive
v0x55acd2b024f0_0 .net "reset", 0 0, o0x7f0ca60320d8;  0 drivers
v0x55acd2b02600_0 .var "state", 1 0;
E_0x55acd2adde20 .event edge, v0x55acd2b02240_0;
E_0x55acd2adda40 .event posedge, v0x55acd2b024f0_0, v0x55acd2ae2160_0;
    .scope S_0x55acd2ae1f30;
T_0 ;
    %wait E_0x55acd2adda40;
    %load/vec4 v0x55acd2b024f0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 2;
    %assign/vec4 v0x55acd2b02600_0, 0;
    %pushi/vec4 3, 3, 2;
    %assign/vec4 v0x55acd2b02320_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x55acd2b02320_0;
    %assign/vec4 v0x55acd2b02600_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x55acd2ae1f30;
T_1 ;
    %wait E_0x55acd2adde20;
    %load/vec4 v0x55acd2b02320_0;
    %assign/vec4 v0x55acd2b02600_0, 0;
    %load/vec4 v0x55acd2b02240_0;
    %assign/vec4 v0x55acd2b02320_0, 0;
    %pushi/vec4 3, 3, 2;
    %cassign/vec4 v0x55acd2b02410_0;
    %load/vec4 v0x55acd2b02240_0;
    %cmpi/e 3, 3, 2;
    %flag_mov 8, 4;
    %load/vec4 v0x55acd2b02240_0;
    %cmpi/e 0, 3, 2;
    %flag_or 4, 8;
    %jmp/0xz  T_1.0, 4;
    %pushi/vec4 3, 3, 2;
    %cassign/vec4 v0x55acd2b02410_0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x55acd2b02240_0;
    %load/vec4 v0x55acd2b02600_0;
    %cmp/e;
    %jmp/0xz  T_1.2, 6;
    %pushi/vec4 2, 0, 2;
    %cassign/vec4 v0x55acd2b02410_0;
    %jmp T_1.3;
T_1.2 ;
    %pushi/vec4 1, 0, 2;
    %cassign/vec4 v0x55acd2b02410_0;
T_1.3 ;
T_1.1 ;
    %jmp T_1;
    .thread T_1, $push;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "simon1.v";
